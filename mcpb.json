{
  "name": "database-operations-mcp",
  "version": "1.0.0",
  "description": "Universal Database Operations MCP - SQL, NoSQL, Vector databases with Windows Registry support",
  "author": "Sandra Schilling",
  "license": "MIT",
  "outputDir": "dist",
  "mcp": {
    "version": "2.12.0",
    "server": {
      "command": "python",
      "args": ["-m", "database_operations_mcp.main"],
      "transport": "stdio"
    },
    "capabilities": {
      "tools": true,
      "resources": true,
      "prompts": true
    }
  },
  "dependencies": {
    "python": ">=3.9.0",
    "fastmcp": ">=2.12.0,<3.0.0"
  }
}

