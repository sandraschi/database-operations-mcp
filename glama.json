{
  "$schema": "https://glama.ai/mcp/schemas/server.json",
  "maintainers": ["sandraschi"],
  "name": "database-operations-mcp",
  "description": "FastMCP 2.14.3 server for database operations on Windows with bookmark tools as secondary feature. 24 portmanteau tools consolidating 124+ operations.",
  "type": "mcp_server",
  "status": "beta",
  "framework": "FastMCP 2.14.3",
  "tools": 23,
  "transport": ["stdio"],
  "keywords": [
    "database",
    "mcp-server",
    "sqlite",
    "postgresql",
    "mysql",
    "bookmarks",
    "firefox",
    "chromium",
    "fastmcp",
    "windows"
  ],
  "features": {
    "database_operations": {
      "enabled": true,
      "databases": ["SQLite", "PostgreSQL", "MySQL", "Windows App Databases"]
    },
    "bookmark_tools": {
      "enabled": true,
      "browsers": ["Firefox", "Chromium", "Edge", "Chrome"],
      "note": "Secondary feature - cross-browser bookmark sync"
    }
  }
}

