{
  "name": "database-operations-mcp",
  "version": "1.0.0",
  "description": "Universal Database Operations MCP - SQL, NoSQL, and Vector database management with Windows Registry support",
  "author": "Sandra",
  "license": "MIT",
  "outputDir": "dist",
  "mcp": {
    "version": "2.10.1",
    "server": {
      "command": "python",
      "args": ["-m", "src.database_operations"],
      "transport": "stdio"
    },
    "capabilities": {
      "tools": true,
      "resources": true,
      "prompts": true
    }
  },
  "fastmcp_compliance": {
    "version": "2.10.1",
    "transport": "stdio",
    "entry_point": "src.database_operations.main:main"
  },
  "dependencies": {
    "python": ">=3.9.0"
  }
}