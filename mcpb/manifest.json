{
  "manifest_version": "0.2",
  "name": "database-operations-mcp",
  "version": "1.2.0",
  "description": "Universal Database Operations MCP - SQL, NoSQL, Vector databases with Windows Registry support",
  "author": {
    "name": "Sandra Schilling",
    "email": "sandra@example.com"
  },
  "license": "MIT",
  "server": {
    "type": "python",
    "entry_point": "src/database_operations_mcp/main.py",
    "mcp_config": {
      "command": "python",
      "args": ["-m", "database_operations_mcp.main"],
      "env": {
        "PYTHONPATH": "${PWD}",
        "PYTHONUNBUFFERED": "1"
      }
    }
  },
  "capabilities": {
    "tools": true,
    "resources": true,
    "prompts": true
  },
  "dependencies": [
    "fastmcp>=2.12.0,<3.0.0",
    "fastapi>=0.95.0",
    "uvicorn[standard]>=0.21.0",
    "pydantic>=2.11.7",
    "psycopg2-binary>=2.9.5",
    "pymongo>=4.3.0",
    "chromadb>=0.4.0",
    "sqlalchemy[asyncio]>=2.0.0"
  ],
  "compatibility": {
    "platforms": ["windows", "macos", "linux"],
    "python_version": ">=3.10"
  }
}
