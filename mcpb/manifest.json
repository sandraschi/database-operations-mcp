{
  "manifest_version": "0.2",
  "name": "database-operations-mcp",
  "version": "1.2.3",
  "description": "Universal Database Operations MCP - SQL, NoSQL, Vector databases with Windows Registry support",
  "author": {
    "name": "Sandra Schilling",
    "email": "sandra@example.com"
  },
  "license": "MIT",
  "server": {
    "type": "python",
    "entry_point": "src/database_operations_mcp/main.py",
    "mcp_config": {
      "command": "python",
      "args": ["-m", "database_operations_mcp.main"],
      "env": {
        "PYTHONPATH": "${PWD}",
        "PYTHONUNBUFFERED": "1"
      }
    }
  },
  "compatibility": {
    "platforms": ["win32", "darwin", "linux"],
    "python_version": ">=3.10"
  }
}
